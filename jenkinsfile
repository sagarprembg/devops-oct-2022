pipeline{
 agent any;
  stages{
   stage('build'){
    steps{
      echo "this is build stage"
      }
   }
   stage('test'){ 
    parallel{
     stage('running on chrome')
     {
     steps{
     echo "running on chrome"
     }
    }
    stage('running on firefox'){
    
    steps{
    echo "this is a test stage"
      }
    }
    }
    stage('deploy'){
     steps{
     echo "this is deploy stage"
     }
    }
   }
}
